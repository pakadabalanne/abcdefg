        <script>
            var xhttp = new XMLHttpRequest();
            xhttp.open("GET" , "k.xml" , true);
            xhttp.send();
            xhttp.onreadystatechange = function(){
            if(xhttp.readyState == 4 && xhttp.status==200){
                var xmlDoc = xhttp.responseXML;
                
                document.getElementById("data1").innerHTML = xmlDoc.getElementsByTagName("ID")[0].childNodes[0].nodeValue;
                document.getElementById("data2").innerHTML = xmlDoc.getElementsByTagName("Name")[0].childNodes[0].nodeValue;
                document.getElementById("data3").innerHTML = xmlDoc.getElementsByTagName("Branch")[0].childNodes[0].nodeValue;
                document.getElementById("data4").innerHTML = xmlDoc.getElementsByTagName("Position")[0].childNodes[0].nodeValue;
                document.getElementById("data5").innerHTML = xmlDoc.getElementsByTagName("Years")[0].childNodes[0].nodeValue;

                document.getElementById("data6").innerHTML = xmlDoc.getElementsByTagName("ID")[1].childNodes[0].nodeValue;
                document.getElementById("data7").innerHTML = xmlDoc.getElementsByTagName("Name")[1].childNodes[0].nodeValue;
                document.getElementById("data8").innerHTML = xmlDoc.getElementsByTagName("Branch")[1].childNodes[0].nodeValue;
