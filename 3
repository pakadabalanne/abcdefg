                document.getElementById("data18").innerHTML = xmlDoc.getElementsByTagName("Branch")[3].childNodes[0].nodeValue;
                document.getElementById("data19").innerHTML = xmlDoc.getElementsByTagName("Position")[3].childNodes[0].nodeValue;
